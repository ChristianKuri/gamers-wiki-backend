{
  "name": "gamers-wiki-backend",
  "version": "0.1.0",
  "private": true,
  "description": "A Strapi application",
  "scripts": {
    "build": "strapi build",
    "console": "strapi console",
    "deploy": "strapi deploy",
    "dev": "strapi develop",
    "develop": "strapi develop",
    "start": "strapi start",
    "strapi": "strapi",
    "upgrade": "npx @strapi/upgrade latest",
    "upgrade:dry": "npx @strapi/upgrade latest --dry",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "RUN_E2E_TESTS=true vitest run --config vitest.e2e.config.ts",
    "test:e2e:env": "lsof -ti :1338 | xargs -r kill -9 2>/dev/null; PORT=1338 DATABASE_NAME=strapi_test npm run develop",
    "test:e2e:run": "bash scripts/run-e2e.sh",
    "test:e2e:article": "bash scripts/run-e2e.sh tests/article-generator/e2e/",
    "test:e2e:article-guide-zelda": "bash scripts/run-e2e.sh tests/article-generator/e2e/guide-zelda.e2e.test.ts",
    "test:e2e:article-guide-bg3": "bash scripts/run-e2e.sh tests/article-generator/e2e/guide-bg3.e2e.test.ts",
    "test:e2e:article-guide-eldenring": "bash scripts/run-e2e.sh tests/article-generator/e2e/guide-eldenring.e2e.test.ts",
    "test:e2e:article-guide-clairobscur": "bash scripts/run-e2e.sh tests/article-generator/e2e/guide-clairobscur.e2e.test.ts",
    "test:e2e:game-import": "bash scripts/run-e2e.sh tests/game-fetcher/e2e/",
    "test:e2e:cleaner": "RUN_E2E_TESTS=true vitest run --config vitest.e2e.config.ts tests/cleaner/e2e/cleaner.e2e.test.ts",
    "test:all": "vitest run && bash scripts/run-e2e.sh",
    "test:ai:game-descriptions": "npx tsx scripts/test-ai-game-descriptions.ts",
    "test:ai:game-resolver": "npx tsx scripts/test-ai-game-resolver.ts"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.76",
    "@openrouter/ai-sdk-provider": "^1.5.4",
    "@sensinum/strapi-plugin-mcp": "^1.1.0",
    "@strapi/plugin-cloud": "5.30.1",
    "@strapi/plugin-users-permissions": "5.30.1",
    "@strapi/provider-upload-aws-s3": "5.30.1",
    "@strapi/strapi": "5.30.1",
    "ai": "^5.0.106",
    "better-sqlite3": "11.3.0",
    "pg": "^8.13.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.0.0",
    "sharp": "^0.34.5",
    "styled-components": "^6.0.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.15",
    "knex": "^3.0.1",
    "msw": "^2.12.4",
    "supertest": "^7.1.4",
    "typescript": "^5",
    "vitest": "^4.0.15"
  },
  "engines": {
    "node": ">=18.0.0 <=22.x.x",
    "npm": ">=6.0.0"
  },
